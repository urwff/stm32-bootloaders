CompileFlags:
  Add:
    - '-ferror-limit=0'
    - '-Wno-implicit-int'
    - '--target=arm-none-eabi'
  CompilationDatabase: build/Debug
  Compiler: arm-none-eabi-gcc
Diagnostics:
  Suppress:
    - unused-includes
    - unknown_typename
    - unknown_typename_suggest
    - typename_requires_specqual
    - pp_file_not_found
    - implicit-function-declaration
